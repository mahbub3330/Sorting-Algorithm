    "#-------------------\n",
    "#Bubble sort\n",
    "#-------------------\n",
    "\n",
    "def bubble_sort(A):\n",
    "    for i in range(len(A)-1,0,-1):\n",
    "        for j in range(i):\n",
    "            if A[j]>A[j+1]:\n",
    "                A[j+1],A[j]=A[j],A[j+1]\n",
    "A=[7,4,2,5,12,1,3]\n",
    "bubble_sort(A)\n",
    "print(A)"
   ]
